   <tu tuid="4" datatype="Text">
    <prop type="score-hunalign">2.54412</prop>
    <prop type="score-bicleaner">0.505</prop>
    <prop type="type">1:1</prop>
    <tuv xml:lang="en">
     <prop type="source-document">https://www.toll.no/en/goods/fish/export-of-fish/fish-and-crustaceans/</prop>
