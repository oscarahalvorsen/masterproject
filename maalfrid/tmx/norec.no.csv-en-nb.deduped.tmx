   </tu>
   <tu tuid="15" datatype="Text">
    <prop type="score-hunalign">1.03776</prop>
    <prop type="score-bicleaner">0.602</prop>
    <prop type="type">1:1</prop>
    <tuv xml:lang="en">
     <prop type="source-document">https://www.norec.no/en/aktuelle/korona-faste-i-norge-og-veldig-langt-hjemmefra-2/</prop>
